{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ],
      "@assets/*": [
        "./assets/*"
      ],
      "@env/*": [
        "./env/*"
      ],
      "@components/*": [
        "./components/*"
      ],
      "@data/*": [
        "./data/*"
      ],
      "@screens/*": [
        "./app/screens*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "class-variance-authority.d.ts",
    "CustomSwitch.js",
    "app/screens/profileManagement",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}
