{
  "expo": {
    "name": "SwiftRide",
    "slug": "frontend_playground",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "SwiftRide"
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.codeplayug.frontend_playground"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "89be462b-4772-4154-8777-33892cd8c3e2"
      }
    },
    "plugins": [
      "expo-router",
      ["@react-native-firebase/app", "@react-native-firebase/auth"],
      [
        "@rnmapbox/maps",
        {
          "RNMapboxMapsVersion": "11.4.0",
          "RNMapboxMapsDownloadToken": "sk.eyJ1IjoiYXNrdGliYSIsImEiOiJjbHpqbnZ6dWMwbmNrMnhzMW5ybWhveWJ5In0.h6X-CLGs7r62shCx1boE6Q"
        }
      ],
      [
        "expo-location",
        {
          "locationWhenInUsePermission": "Show current location on map."
        }
      ]
    ]
  }
}
